    {
      "name": "aklump",
      "email": "sourcecode@intheloftstudios.com"
    }
  ],
  "require": {
    "aklump/loft-lib": "^1|^2",
    "vlucas/phpdotenv": "^2.4",
    "league/color-extractor": "^0.3.2",
    "vimeo/vimeo-api": ">=3",
    "owen-oj/laravel-getid3": "^1.3",
